{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "@/interfaces": [
        "src/interfaces/*"
      ],
      "@/assets": [
        "src/assets/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/routes/*": [
        "src/routes/*"
      ],
      "@/screens/*": [
        "src/screens/*"
      ],
      "@/storage/*": [
        "src/storage/*"
      ],
      "@/theme/*": [
        "src/theme/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]
    }
  }
}